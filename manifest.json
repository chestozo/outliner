{
    "manifest_version": 2,

    "name": "Chrome plugins opener",
    "version": "0.1",
    "description": "",

    "background": {
        "scripts": [
            "jquery.min.js",
            "bg.js"
        ]
    },

    "icons": {
        "16": "favicon.16.png"
    },

    "browser_action": {
        "default_icon": "favicon.19.png"
    },

    "permissions": [
        "*://*/*",
        "tabs",
        "contextMenus"
    ]
}
